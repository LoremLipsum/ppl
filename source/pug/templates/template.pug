include ./include

block vars
    - var pageTitle = 'ТД Время'
    - var main = false

doctype html
html(lang="ru-RU")
    block head
        +head()

    body
        block icon
        block header
            if main
                +header({})
            if !main
                +page-header({})
        block content
        block footer
            +footer({})
        block modal

        block video
        block scripts
            +scripts()
